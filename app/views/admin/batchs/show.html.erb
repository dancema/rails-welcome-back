<% @starcodes.each_with_index do |starcode, index| %>
  <div class="card-flyer">
    <h2><%= starcode.code %></h2>
    <table class="qr">
      <% @qr[index].modules.each_index do |x| %>
          <tr>
            <% @qr[index].modules.each_index do |y| %>
                <% if @qr[index].qrcode.checked?(x,y) %>
                    <td class="black"/>
                <% else %>
                    <td class="white"/>
                <% end %>
            <% end %>
          </tr>
      <% end %>
    </table>
  </div>
<% end %>
